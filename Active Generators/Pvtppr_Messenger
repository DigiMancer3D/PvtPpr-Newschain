 function ranger() {
   var xd = document.getElementById("myRange");
  if (xd.value === "1") {
    xd.value = "2";
   document.getElementById('myRange').innerHTML = "Add Authentication Message";
   document.getElementById('myRange').style = "background-color:skyblue;color:darkblue;border-color:blue; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
   document.getElementById('autmsg').style = "display:none;";
   document.getElementById('autmsg').value = "Made with PvtPpr Lite Generator";
   document.getElementById('hr006').style = "display:none;";
   document.getElementById('hr030').style = "display:none;";
  } else {
    xd.value = "1";
   document.getElementById('myRange').innerHTML = "Remove Authentication Message";
   document.getElementById('myRange').style = "background-color:lightgreen;color:darkgreen;border-color:green; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
    document.getElementById('autmsg').style = "display:inline-block; width:98%; padding:0.5vw;text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('autmsg').value = "";
   document.getElementById('hr006').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   document.getElementById('hr030').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
  }
  }
