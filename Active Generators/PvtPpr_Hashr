    function colormath() {
     var redbuild = Math.floor((Math.random() * 255)+0);
     var greenbuild = Math.floor((Math.random() * 255)+0);
     var bluebuild = Math.floor((Math.random() * 255)+0);
     document.getElementById('redsave').innerHTML = redbuild;
     document.getElementById('bluesave').innerHTML = bluebuild;
     document.getElementById('greensave').innerHTML = greenbuild;
     var numred = document.getElementById('rednum').innerHTML;
     var numgreen = document.getElementById('greennum').innerHTML;
     var numblue = document.getElementById('bluenum').innerHTML;
     var red2 = redbuild - numred;
     var green2 = greenbuild - numgreen;
     var blue2 = bluebuild - numblue;
     document.getElementById('redsave').innerHTML = red2;
     document.getElementById('bluesave').innerHTML = blue2;
     document.getElementById('greensave').innerHTML = green2;
     pickrndc();
    }

    
   </script>
   <script>
    function theme2() {
     var theme = document.getElementById('theme2');
     if (theme.value === '1') {
      theme.value = '2';
      document.getElementById('body').style = "text-size:148%; margin-top:-1.5vw; background-color:white; color: black;text-align:left;align:left; word-wrap: break-word;scroll-behavior:smooth; scroll-y:smooth; javascript: allow;";
      document.getElementById('world').style = 'background-color:transprent;';
      document.getElementById('theme2').innerHTML = " 🔅 ";
      document.getElementById('headercolor').style = 'position:fixed;top:0;left:0;background: transparent; background: linear-gradient(0deg, transparent 0%, white 25%);';
     } else {
      theme.value = '1';
      document.getElementById('body').style = "text-size:148%; margin-top:-1.5vw; background-color:black; color: white;text-align:left;align:left; word-wrap: break-word;scroll-behavior:smooth; scroll-y:smooth; javascript: allow;";
      document.getElementById('world').style = 'background-color:transparent;';
      document.getElementById('theme2').innerHTML = " 🔆 ";
      document.getElementById('headercolor').style = 'position:fixed;top:0;left:0; background: linear-gradient(0deg, transparent 0%, black 25%);';
     }
    }
   </script>
   </div>
  </span>
