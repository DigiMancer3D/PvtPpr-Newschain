    function contentsend() {
     var inhold = document.getElementById('inin').value;
     document.getElementById('extrahold').innerHTML += inhold;
     document.getElementById('contentholder').innerHTML += inhold+'</br>';
     document.getElementById('extra2hold').innerHTML += document.getElementById('inin').value+' ';
     document.getElementById('btn6').innerHTML = '<i style="color:skyblue;"><b>Done!</b></i>';
     document.getElementById('lights').innerHTML += '&nbsp;🟢&nbsp;';
     document.getElementById('sparehold').innerHTML = document.getElementById('lights').innerHTML;
     document.getElementById('inin').placeholder = '...Submitted....Submit Another? What was sent: "'+inhold+'"    Whats Locked-in: '+document.getElementById('extra2hold').innerHTML+'"         ;}  ';
     document.getElementById('inin').value = '';
     document.getElementById('previewbottom').innerHTML += "<span align='center' style='width:100%;'>Content Block:</br>"+document.getElementById('contentholder').innerHTML+"</span>";
    }
    
