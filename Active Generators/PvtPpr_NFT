 function mone() {
   var xd = document.getElementById("bitmone");
  if (xd.value === "1") {
    xd.value = "2";
   document.getElementById('bitmone').innerHTML = "Add Bittube Monetization";
   document.getElementById('bitmone').style = "background-color:skyblue;color:darkblue; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
   document.getElementById('bittubeHash').style = "display:none;";
   document.getElementById('bittubeHash').value = "";
   document.getElementById('hr010').style = "display:none;";
   document.getElementById('bittubeName').style = "display:none;";
   document.getElementById('bittubeName').value = "";
   document.getElementById('hr020').style = "display:none;";

  } else {
    xd.value = "1";
   document.getElementById('bitmone').innerHTML = "Remove Bittube Monetization";
   document.getElementById('bitmone').style = "background-color:lightgreen;color:darkgreen; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
    document.getElementById('bittubeHash').style = "display:inline-block; width:98%; padding:0.5vw; text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('bittubeHash').value = "";
   document.getElementById('hr010').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   document.getElementById('bittubeName').style = "display:inline-block; width:98%; padding:0.5vw; text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('bittubeName').value = "";
   document.getElementById('hr020').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   
document.getElementById("bittubeHash").addEventListener("keyup", function(event) {
  // Number 13 is the "Enter" key on the keyboard
  if (event.keyCode === 13) {
    // Cancel the default action, if needed
    event.preventDefault();
   // focus next input
    document.getElementById("bittubeName").focus();
}});
document.getElementById("bittubeName").addEventListener("keyup", function(event) {
  // Number 13 is the "Enter" key on the keyboard
  if (event.keyCode === 13) {
    // Cancel the default action, if needed
    event.preventDefault();
   // focus next input
    document.getElementById("previn").focus();
}});
  }
 }
 
</script>
