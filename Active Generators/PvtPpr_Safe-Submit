 function ranger() {
   var xd = document.getElementById("myRange");
  if (xd.value === "1") {
    xd.value = "2";
   document.getElementById('myRange').innerHTML = "Increase Paper Security";
   document.getElementById('myRange').style = "background-color:skyblue;color:darkblue;border-color:blue; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
   document.getElementById('autmsg').style = "display:none;";
   document.getElementById('autmsg').value = "Made with PvtPpr Lite Generator";
   document.getElementById('hr006').style = "display:none;";
   document.getElementById('certlink').style = "display:none;";
   document.getElementById('certlink').value = "https://3dd.in/ppgen";
   document.getElementById('hr030').style = "display:none;";
   
  } else {
    xd.value = "1";
   document.getElementById('myRange').innerHTML = "Decrease Paper Security";
   document.getElementById('myRange').style = "background-color:lightgreen;color:darkgreen;border-color:green; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
    document.getElementById('autmsg').style = "display:inline-block; width:98%; padding:0.5vw;text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('autmsg').value = "";
   document.getElementById('hr006').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   document.getElementById('certlink').style = "display:inline-block; left:0; width:98%; padding:0.5vw;text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('certlink').value = "";
   document.getElementById('hr030').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
  
document.getElementById("autmsg").addEventListener("keyup", function(event) {
  // Number 13 is the "Enter" key on the keyboard
  if (event.keyCode === 13) {
    // Cancel the default action, if needed
    event.preventDefault();
   // focus next input
    document.getElementById("btc").focus();
}});
  }
  }
 
 function mone() {
   var xd = document.getElementById("bitmone");
  if (xd.value === "1") {
    xd.value = "2";
   document.getElementById('bitmone').innerHTML = "Add Bittube Monetization";
   document.getElementById('bitmone').style = "background-color:skyblue;color:darkblue; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
   document.getElementById('bittubeHash').style = "display:none;";
   document.getElementById('bittubeHash').value = "";
   document.getElementById('hr010').style = "display:none;";
   document.getElementById('bittubeName').style = "display:none;";
   document.getElementById('bittubeName').value = "";
   document.getElementById('hr020').style = "display:none;";

  } else {
    xd.value = "1";
   document.getElementById('bitmone').innerHTML = "Remove Bittube Monetization";
   document.getElementById('bitmone').style = "background-color:lightgreen;color:darkgreen; cursor: pointer; padding:0.5vw; font-size: 90%; font-weight: bold; box-shadow: 3px 2px #223322;";
    document.getElementById('bittubeHash').style = "display:inline-block; width:98%; padding:0.5vw; text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('bittubeHash').value = "";
   document.getElementById('hr010').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   document.getElementById('bittubeName').style = "display:inline-block; width:98%; padding:0.5vw; text-align:center; border: none; background-color: #cbaccc; font-size:148%; z-index:1; color: #3a253b;";
   document.getElementById('bittubeName').value = "";
   document.getElementById('hr020').style = "display:block; border-width:0.6px; border-color:#cbaccc; color:#cbaccc; z-index:3;";
   
document.getElementById("bittubeHash").addEventListener("keyup", function(event) {
  // Number 13 is the "Enter" key on the keyboard
  if (event.keyCode === 13) {
    // Cancel the default action, if needed
    event.preventDefault();
